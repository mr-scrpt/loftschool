.me{
  &__inner{
    display: flex;
    max-width: 640px;
  }
  &__info{
    font-size: 18px;
    text-align: right;
    @include to(#fff,.4);
    padding-right: 30px;
    min-width: 170px;
  }
  &__name{
    color: #fff;
    font-size: 72px;
    font-weight: 800;
    line-height: 60px;
    padding: 0 0 0 30px;
    margin: 0;
  }
}