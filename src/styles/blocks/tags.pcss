.tags{
  &__inner{
    display: flex;
    flex-wrap: wrap;
  }
  &__item{
    @include bgc(#f1f5f9);
    padding: 10px 18px;
    margin: 0 15px 0 0;
    border-radius: 15px;
    cursor: pointer;
    @include ts(background,color);
    &:hover{
      @include bgc($orange);
      color: #fff;
    }
  }
}